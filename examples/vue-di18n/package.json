  "description": "vue-di18n",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.conf.js",
    "prod": "cross-env NODE_ENV=production webpack --config build/webpack.build.conf.js",
    "test": "cross-env NODE_ENV=testing webpack --config build/webpack.build.conf.js",
    "lint": "eslint --ext .vue --ext .js src/ --fix",
    "analyz": "npm_config_report=true npm run prod",
    "di18n:init": "di18n init --vue",
    "di18n:sync": "di18n sync"
  },
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
